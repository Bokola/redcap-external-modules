{
	"name": "Example Module",
	
	"permissions": [
		"hook_redcap_add_edit_records_page",
		"select_data",
		"update_data",
		"delete_data",
		"update_user_permissions"
	],

	"application-links": {
		"My Module's Custom Application Link": "project_homepage.php",
		"Another Custom Application Link": "module_subdirectory/some_other_page.php"
	},

	"global-settings": {
		"favorite-color": {
			"name": "Favorite Color",
			"required": false,
			"allow-project-overrides": false,
			"type": "text",
			"default": "Blue"
		},
		"preferred-transporation": {
			"name": "Preferred Method of Transportation",
			"required": true,
			"allow-project-overrides": true,
			"type": "radio",
			"choices": "car, Car\nbus, Bus\nbike, Bicycle"
		}
	},
	
	"project-settings": {
		"literate": {
			"name": "I certify that I am literate",
			"required": true,
			"type": "checkbox"
		},
		"date-of-birth": {
			"name": "Date of Birth",
			"required": true,
			"type": "text",
			"validation-type": "date_dmy",
			"validation-min": "1/1/1900",
			"validation-max": "1/1/2000"
		},
		"likes-pizza": {
			"name": "Do you like pizza?",
			"required": false,
			"type": "dropdown",
			"choices": "1, Yes\n2, No"
		}
	}
}